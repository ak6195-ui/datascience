   "source": [
    "# Method Overriding in Python"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "8a90bc23-a129-4a20-999a-ef0918465422",
   "metadata": {},
   "outputs": [],
   "source": [
    "class Animal:\n",
    "    def sound(self):\n",
    "        print(\"making sound\")\n",
    "        "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "id": "dcf72116-3e38-49a5-a30e-13c65db307ec",
   "metadata": {},
   "outputs": [],
   "source": [
    "class Dog(Animal):\n",
    "    def sound(self):\n",
    "        super().sound()\n",
    "        print(f\"Dog makes woof\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "id": "3b5271d3-9c3a-4e9f-9f2b-7aecd2fbe193",
   "metadata": {},
   "outputs": [],
   "source": [
    "dog= Dog()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "id": "df999f83-d08f-478d-88ae-e359f095fb12",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "making sound\n",
      "Dog makes woof\n"
     ]
    }
   ],
   "source": [
    "dog.sound()"
   ]
  },
